{
  "manifest_version": 2,

  "name": "Meme Generator",
  "description": "Generate memes like a boss.",
  "version": "1.0",

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://*/",
    "http://*.memeful.com/"
  ]
}
